{
  "github": {
    "release": true,
    "releaseName": "Release v${version}",
    "preRelease": false,
    "draft": false,
    "tokenRef": "GITHUB_TOKEN",
    "assets": "",
    "timeout": 0,
    "skipChecks": true
  },
  "hooks": {
    "after:bump": [
      "nimble version v${version}",
      "nimble docgen"
    ]
  }
}